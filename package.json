    "is-network-error": "^1.0.1",
    "ky": "^1.2.0",
    "nanoid": "^5.0.4",
    "react-hot-toast": "^2.4.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "peerDependencies": {
    "@types/react": ">=16.14.8",
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
