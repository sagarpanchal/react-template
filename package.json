{
  "name": "react-boiler",
  "private": true,
  "version": "1.0.5",
  "type": "module",
  "main": "es/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "scripts": {
    "prepare": "husky",
    "start": "npm run start:development",
    "start:development": "vite --mode development",
    "start:staging": "vite --mode staging",
    "start:production": "vite --mode production",
    "build": "npm run build:development",
    "build:package": "tsc && vite build --mode package",
    "build:development": "tsc && vite build --mode development",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build --mode production",
    "preview": "vite preview",
    "lint:fix": "stylelint --fix \"src/**/*.{css,scss}\" && prettier --write \"src/**/*.{ts,tsx,js,jsx,md,json}\" && eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "tsc && eslint \"src/**/*.{ts,tsx,js,jsx}\" && stylelint \"src/**/*.{css,scss}\"",
    "test": "vitest --run",
    "test:ui": "vitest --ui --coverage",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "volta": {
    "node": "20.11.0",
    "npm": "10.3.0"
  },
  "lint-staged": {
    "!(es)/**/*.{css,scss}": [
      "stylelint --fix"
    ],
    "!(es)/**/*.{md,json}": [
      "prettier --write"
    ],
    "!(es)/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
