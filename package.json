    "vite-plugin-dts": "^3.7.2",
    "vite-plugin-node-polyfills": "^0.19.0",
    "vite-plugin-stylelint": "^5.3.1",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.1",
    "write-json-file": "^5.0.0"
  }
}
